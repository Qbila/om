{
  "name": "javascript-development-environment",
  "version": "1.0.0",
  "description": "JavaScript development environment",
  "scripts": {
    "build-dev": "webpack --config webpack.config.dev.js",
    "build-prod": "webpack --config webpack.config.prod.js",
    "coverage": "babel-istanbul cover _mocha scripts/testSetup.js 'modules/**/*.test.js' 'public/js/**/*.test.js'",
    "devStart": "npm-run-all --parallel security-check build-dev initServer lint:watch test:watch",
    "initServer": "babel-node server.js",
    "lint": "esw webpack.config.* scripts server client server.js --color",
    "lint:watch": "npm run lint -- --watch",
    "localtunnel": "lt --port 3000",
    "precoverage": "rm -rf coverage",
    "predevStart": "npm-run-all --parallel prestartjs removeBuild lint test",
    "prestart": "npm-run-all --parallel prestartjs removeBuild build-prod",
    "prestartjs": "babel-node scripts/preStart.js",
    "test": "mocha scripts/testSetup.js 'modules/**/*.test.js' 'public/js/**/*.test.js'",
    "test:watch": "npm run test -- --watch",
    "removeBuild": "rm -rf public/dist",
    "security-check": "nsp check",
    "share": "npm-run-all --parallel localtunnel",
    "start": "npm-run-all --parallel security-check initServer lint test"
  },
  "author": "Cory House",
  "contributors": [
    {
      "name": "Ashish Rana"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "angular": "^1.6.6",
    "env2": "^2.2.0",
    "handlebars": "^4.0.8",
    "hapi": "^16.1.1",
    "hapi-alive": "^1.4.0",
    "hapi-swagger": "^7.7.0",
    "inert": "^4.2.0",
    "joi": "^10.4.2",
    "jquery": "^3.2.1",
    "jshint": "^2.9.5",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mongodb": "^2.2.28",
    "node-loggly-bulk": "^2.0.0",
    "path": "^0.12.7",
    "randomstring": "^1.1.5",
    "request": "^2.81.0",
    "useragent": "^2.1.13",
    "vision": "^4.1.1",
    "zingchart": "^2.6.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.1",
    "chalk": "^2.1.0",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.5",
    "eslint": "^4.5.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-watch": "^3.1.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "hapi-webpack-plugin": "^2.0.0",
    "html-webpack-plugin": "^2.30.1",
    "istanbul": "^0.4.5",
    "jsdom": "^11.2.0",
    "json-schema-faker": "^0.4.3",
    "json-server": "^0.12.0",
    "localtunnel": "^1.8.3",
    "mocha": "^3.5.0",
    "nock": "^9.0.14",
    "npm-run-all": "^4.1.1",
    "nsp": "^2.7.0",
    "numeral": "^2.0.6",
    "open": "0.0.5",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-md5-hash": "0.0.5"
  }
}
